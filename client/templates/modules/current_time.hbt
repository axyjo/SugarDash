<h1 class="module_title">Current Time</h2>
<div class="widget" id="widget_time_cupertino" data-func="app.time" data-tz="America/Los_Angeles">
    {{#if widget_time_cupertino}}
        <div class="item"><h3>Cupertino (Pacific): <span class="datetime">{{widget_time_cupertino.datetime}}</span></h3></div>
    {{/if}}
</div>

<div class="widget" id="widget_time_durham" data-func="app.time" data-tz="America/New_York">
    {{#if widget_time_durham}}
        <div class="item"><h3>Durham, NC (Eastern): <span class="datetime">{{widget_time_durham.datetime}}</span></h3></div>
    {{/if}}
</div>

