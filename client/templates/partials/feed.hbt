<div class="widget">
<div class="widget_title">{{this.title}}</div>
{{#each this.entries}}
<div class="feed_story">
    <div class="feed_story_title">{{this.title}}</div>
    <div class="feed_story_time"><span class="moment_datetime">{{this.publishedDate}}</span></div>
    <div class="feed_story_snippet">{{this.contentSnippet}}</div>
</div>
{{/each}}

</div>

