<div class="item widget_title">{{this.title}}</div>
{{#each this.entries}}
<div class="feed_story item">
    <h3 class="feed_story_title">{{this.title}}</h3>
    <div class="feed_story_time"><span class="moment_datetime">{{this.publishedDate}}</span></div>
    <div class="feed_story_snippet">
        {{{this.content}}}
    </div>
</div>
{{/each}}
